{
  "name": "sheep-ui",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.tsx,.vue src",
    "prepare": "husky install",
    "test": "vitest"
